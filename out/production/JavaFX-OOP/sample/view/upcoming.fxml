<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="885.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.Upcoming">
    <children>
        <ImageView fitHeight="505.0" fitWidth="885.0" layoutY="-25.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <image>
                <Image url="@../image/alatooimg.jpg" />
            </image>
        </ImageView>
        <AnchorPane prefHeight="480.0" prefWidth="885.0" style="-fx-background-color: #001642; -fx-opacity: 0.6;" />
        <AnchorPane layoutX="135.0" prefHeight="480.0" prefWidth="750.0" style="-fx-background-color: #000000; -fx-opacity: 0.4;" />
        <AnchorPane prefHeight="480.0" prefWidth="135.0">
            <children>
                <AnchorPane prefHeight="120.0" prefWidth="135.0" style="-fx-border-color: #000000;">
                    <children>
                        <ImageView fitHeight="90.0" fitWidth="90.0" layoutX="22.0" layoutY="15.0" onMouseClicked="#handlerHome" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 0.7;">
                            <image>
                                <Image url="@../image/home.png" />
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
                <AnchorPane layoutY="120.0" prefHeight="120.0" prefWidth="135.0" style="-fx-border-color: #000000;">
                    <children>
                        <ImageView fitHeight="90.0" fitWidth="90.0" layoutX="22.0" layoutY="15.0" onMouseClicked="#handlerHistory" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 0.7;">
                            <image>
                                <Image url="@../image/history.png" />
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
                <AnchorPane layoutY="240.0" prefHeight="120.0" prefWidth="135.0" style="-fx-border-color: #000000;">
                    <children>
                        <ImageView fitHeight="90.0" fitWidth="90.0" layoutX="22.0" layoutY="15.0" onMouseClicked="#handlerPatient" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 0.7;">
                            <image>
                                <Image url="@../image/add_task.png" />
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
                <AnchorPane layoutY="360.0" prefHeight="120.0" prefWidth="135.0" style="-fx-border-color: #000000;">
                    <children>
                        <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="17.0" layoutY="10.0" onMouseClicked="#handlerUpcoming" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 1;">
                            <image>
                                <Image url="@../image/notification.png" />
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="135.0" prefHeight="480.0" prefWidth="750.0">
            <children>
            <TableView fx:id="tableUpc" prefHeight="480.0" prefWidth="750.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <columns>
                <TableColumn fx:id="timeUpc" prefWidth="130.0" text="Time" />
                <TableColumn fx:id="nameUpc" prefWidth="115.0" text="Name" />
                  <TableColumn fx:id="surnameUpc" prefWidth="115.0" text="Surname" />
                  <TableColumn fx:id="facultyUpc" prefWidth="100.0" text="Faculty" />
                  <TableColumn fx:id="numberUpc" prefWidth="115.0" text="Number" />
                  <TableColumn fx:id="diseaseUpc" prefWidth="175.0" text="Disease" />
              </columns>
            </TableView>
            <ImageView fx:id="addBtn" fitHeight="90.0" fitWidth="90.0" layoutX="630.0" layoutY="360.0" onMouseClicked="#handleAdd" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/plus.png" />
               </image>
            </ImageView>
            <ImageView fx:id="disableBtn" fitHeight="90.0" fitWidth="90.0" layoutX="30.0" layoutY="360.0" onMouseClicked="#handleDisable" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/remove.png" />
               </image>
            </ImageView>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
